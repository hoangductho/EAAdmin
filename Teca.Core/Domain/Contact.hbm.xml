<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="Teca.Core.Domain.Contact, Teca.Core" table="Contact" >
    <id name="Id" column="Id" type="Int32" unsaved-value="0">
      <generator class="identity" />
    </id>    
    <property name="Name" column="Name" type="string" length="255" />
    <property name="Email" column="Email" type="string" length="255" />
    <property name="Phone" column="Phone" type="string" length="50" />
    <property name="Address" column="Address" type="string" length="200" />
    <property name="Descriptions" column="Descriptions" type="string" length="1024" />
    <property name="Active" column="Active" type="Boolean" />
    <property name="CreatedDate" column="CreatedDate" type="DateTime" />
    <property name="Type" column="Type" type="Boolean"/>
  </class>

</hibernate-mapping>
