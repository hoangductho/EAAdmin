<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="Teca.Core.Domain.Notification, Teca.Core" table="Notification" dynamic-update="true">
    <id name="Id" column="Id" type="Int32" unsaved-value="0">
      <generator class="identity" />
    </id>
    <property name="ArticleId" column="ArticleId" type="int"/>
    <property name="Status" column="Status" type="int"/>
    <property name="Description" column="Description" type="string" length="1024" />
    <property name="CreateBy" column="CreateBy" type="string" length="50" />
    <property name="ApproveBy" column="ApproveBy" type="string" length="50" />    
    <property name="CreateDate" column="CreateDate" type="DateTime" />    
  </class>

</hibernate-mapping> 
